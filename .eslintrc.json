{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "indent": [2, 4],
    "import/prefer-default-export": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0
  }
}
